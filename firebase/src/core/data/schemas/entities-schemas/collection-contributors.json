{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "memo://memo.schemas/collection-contributors",
  "title": "Collection Contributors",
  "description": "Properties of a Collection Contributor.",
  "type": "object",
  "properties": {
    "name": { "type": "string", "minLength": 1 },
    "url": { "type": "string", "minLength": 1, "format": "url" },
    "avatarUrl": { "type": "string", "minLength": 1, "format": "url" }
  },
  "required": [
    "name", "url", "avatarUrl"
  ]
}